{
  "name": "dotenv-checker",
  "version": "1.1.6",
  "description": "ðŸ”Œ CLI to check/generate .env files and maintain them synchronized",
  "main": "./src/index.js",
  "scripts": {
    "start": "node .",
    "start:watch": "nodemon",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:copy-paste": "jscpd .",
    "lint-fix": "npm run lint -- --fix"
  },
  "bin": {
    "dotenv-checker": "./src/bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chempogonzalez/dotenv-checker.git"
  },
  "keywords": [
    "dotenv",
    "config",
    "environment",
    "checker",
    "generator",
    "conf",
    "env",
    "file",
    "check",
    "sync"
  ],
  "author": "chempogonzalez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chempogonzalez/dotenv-checker/issues"
  },
  "homepage": "https://github.com/chempogonzalez/dotenv-checker#readme",
  "devDependencies": {
    "@chempo/ec-linter": "1.0.9",
    "jscpd": "3.4.5",
    "nodemon": "2.0.15"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "app-root-path": "^3.0.0",
    "chalk": "4.1.2",
    "commander": "9.1.0",
    "gradient-string": "2.0.0",
    "inquirer": "8.2.1"
  },
  "jscpd": {
    "mode": "mild",
    "reporters": [
      "console",
      "time"
    ],
    "ignore": [
      "**/__tests__/**",
      "**/__mocks__/**",
      "**/package.json"
    ],
    "absolute": true,
    "gitignore": true
  }
}
